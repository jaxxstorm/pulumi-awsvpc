{
    "name": "awsvpc",
    "license": "MPLv2",
    "resources": {
        "awsvpc:index:Vpc": {
            "isComponent": true,
            "inputProperties": {
                "availabilityZoneNames": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "List of availability zones to use for your VPC.",
                    "plain": true
                },
                "baseCidr": {
                    "type": "string",
                    "description": "The primary CIDRv4 block to be associated with the VPC.",
                    "plain": true
                },
                "zoneName": {
                    "type": "string",
                    "description": "The name of the private zone to create if createPrivateZone is set to true."
                },
                "createPrivateZone": {
                    "type": "boolean",
                    "description": "Whether to create a private hosted zone attached to the VPC.",
                    "plain": true
                },
                "tags": {
                    "type": "object",
                    "description": "Tags to be applied to each created resource."
                },
                "enableS3Endpoint": {
                    "type": "boolean",
                    "description": "Whether to add the S3 endpoint to the VPC."
                },
                "enableDynamoDBEndpoint": {
                    "type": "boolean",
                    "description": "Whether to add the DynamoDB endpoint to the VPC."
                }
            },
            "requiredInputs": [
                "baseCidr",
                "availabilityZoneNames"
            ],
            "properties": {
                "vpcId": {
                    "type": "string",
                    "description": "The AWS-assigned ID of the VPC"
                },
                "publicSubnetIds": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "The IDs of the created public subnets"
                },
                "privateSubnetIds": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "The IDs of the created private subnets"
                }
            },
            "required": [
                "vpcId",
                "publicSubnetIds",
                "privateSubnetIds"
            ]
        }
    },
    "language": {
        "csharp": {
            "packageReferences": {
                "Pulumi": "3.*",
                "Pulumi.Aws": "4.*"
            }
        },
        "go": {
            "generateResourceContainerTypes": true,
            "importBasePath": "github.com/jaxxstorm/pulumi-vpc/sdk/go/vpc"
        },
        "nodejs": {
            "dependencies": {
                "@pulumi/aws": "^4.0.0"
            },
            "devDependencies": {
                "typescript": "^3.7.0"
            },
            "packageName": "@jaxxstorm/pulumi-awsvpc"
        },
        "python": {
            "requires": {
                "pulumi": ">=3.0.0,<4.0.0",
                "pulumi-aws": ">=4.0.0,<5.0.0"
            }
        }
    }
